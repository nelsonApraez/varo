@model Varo.Web.Models.SolucionesViewModel

<div class="table-wrapper">
    <div class="table-title">
        <div class="row">
            <div class="col m12 l12">
                <h2><strong>@Recursos.Lenguaje.ConsultoriaRelacionada</strong></h2>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col m4 l4">
            <div class="form-group">
                <label for="ListaConsultoria">@Recursos.Lenguaje.Nombre</label>
                @Html.DropDownListFor(model => model.Consultoria,
                    Model.ListaConsultoria.OrderBy(x => x.Text),
                    new { @class = "form-control" })
            </div>
        </div>
    </div>
</div>