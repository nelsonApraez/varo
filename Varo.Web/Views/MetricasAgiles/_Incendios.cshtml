@model Varo.Web.Models.MetricasViewModel
<div class="table-title">
    <div class="row">
        <div class="col m12 l12">
            <h2><strong>@Recursos.Lenguaje.Incendios</strong></h2>
        </div>
    </div>
</div>
<div class="row ">
    <div class="col m4 l4">
        <div class="form-group">
            <label for="Reportados">@Recursos.Lenguaje.Reportados</label>
            @Html.EditorFor(model => model.incendiosViewModel.ReportadosIncendios,
                new { htmlAttributes = new { @class = "form-control validarEnteros" } })
            @Html.ValidationMessageFor(model => model.incendiosViewModel.ReportadosIncendios, null,
                new { @class = "text-danger" })
        </div>
    </div>
    <div class="col m4 l4">
        <div class="form-group">
            <label for="Solucionados">@Recursos.Lenguaje.Solucionados</label>
            @Html.EditorFor(model => model.incendiosViewModel.SolucionadosIncendios,
                new { htmlAttributes = new { @class = "form-control validarEnteros" } })
            @Html.ValidationMessageFor(model => model.incendiosViewModel.SolucionadosIncendios, null,
                new { @class = "text-danger" })
        </div>
    </div>
    <div class="col m4 l4">
        <div class="form-group">
            <label for="Observaciones">@Recursos.Lenguaje.Observaciones</label>
            @Html.EditorFor(model => model.incendiosViewModel.ObservacionesIncendios,
                new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.incendiosViewModel.ObservacionesIncendios, null,
                new { @class = "text-danger" })
        </div>
    </div>
</div>